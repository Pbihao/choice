@font-face {
    font-family: "Comfortaa-Light";
    src: url("Comfortaa-Light.eot"); /* IE9 */
    src: url("Comfortaa-Light.eot?#iefix") format("embedded-opentype"), /* IE6-IE8 */
    
    url(data:application/x-font-ttf;charset=utf-8;base64,) format("truetype"), /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */
    
    url("Comfortaa-Light.svg#Comfortaa-Light") format("svg"); /* iOS 4.1- */
    font-style: normal;
    font-weight: normal;
}


